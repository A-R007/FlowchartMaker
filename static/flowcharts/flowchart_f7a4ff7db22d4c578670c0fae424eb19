digraph CardiacOutput {
  rankdir=LR;
  node[shape=box, style=filled];
  start_node[start];
  end_node[end];
  color_start[start] = light blue;
  color_end[end] = light blue;
  color_process[process] = green;
  color_decision[decision] = red;
  color_edge[edge] = blue;

  start_node -> process_node1[label="Heart Rate (HR)"];
  process_node1[label="HR (bpm)"];
  process_node1[style=filled];
  color_process[process_node1] = green;

  process_node1 -> decision_node1[label="Stroke Volume (SV)"];
  decision_node1[label="SV > 60 mL"];
  decision_node1[shape=box, style=filled];
  color_decision[decision_node1] = red;

  decision_node1 -> process_node2[label="Yes", color=blue];
  decision_node1 -> decision_node2[label="No", color=blue];
  process_node2[label="SV < 60 mL"];
  process_node2[style=filled];
  color_process[process_node2] = green;

  process_node2 -> decision_node3[label="Arterial Blood Pressure (ABP)"];
  decision_node3[label="ABP < 90 mmHg"];
  decision_node3[shape=box, style=filled];
  color_decision[decision_node3] = red;

  decision_node3 -> process_node3[label="No", color=blue];
  decision_node3 -> process_node4[label="Yes", color=blue];
  process_node3[label="Right Atrial Pressure (RAP)"];
  process_node3[style=filled];
  color_process[process_node3] = green;

  process_node4[label="Left Atrial Pressure (LAP)"];
  process_node4[style=filled];
  color_process[process_node4] = green;

  process_node4 -> process_node5[label="Cardiac Output (CO)"];
  process_node5[label="CO (L/min)"];
  process_node5[style=filled];
  color_process[process_node5] = green;

  process_node5 -> end_node[label="End"];
}
