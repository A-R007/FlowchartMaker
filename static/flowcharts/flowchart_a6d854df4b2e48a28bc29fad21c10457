digraph Cardiac_Output {
    node[shape=box, style=filled];
    edge[arrowhead=vee];

    start[label="Start"];
    start->process[label=" cardiac output (CO) =", color=blue];
    process[label="CO = HR x SV", color=blue];
    process->CO[label=" cardiac output (CO) is calculated as the product of heart rate (HR) and stroke volume (SV)", color=blue];
    CO->SVHR[label="Determine factors affecting CO:", color=yellow];
    SVHR->HR[label="Determine effects of heart rate (HR) on CO:", color=red];
    HR->rate[label="Factors affecting HR:", color=red];
    rate->label["1. Sympathetic and parasympathetic nervous system", color=red];
    rate->label["2. Heart block and bundle branch blocks", color=red];
    rate->next->label["3. Age and fitness level", color=red];
    HR->next->response[label="Effects of HR on CO:", color=red];
    response->label["Increases CO as HR increases", color=red];
    SVHR->SV[label="Determine effects of stroke volume (SV) on CO:", color=red];
    SV->volume[label="Factors affecting SV:", color=red];
    volume->label["1. Preload (venous return)", color=red];
    volume->label["2. Afterload (arterial pressure)", color=red];
    volume->label["3. Inotropism (contraction strength)", color=red];
    volume->next->relaxation[label="Effects of SV on CO:", color=red];
    relaxation->label["Increases CO as SV increases", color=red];
    SVHR->pathologies[label="Common pathologies affecting CO:", color=red];
    pathology->label["1. Heart failure", color=red];
    pathology->label["2. Cardiac tamponade", color=red];
    pathology->label["3. MI", color=red];
    SVHR->next->end[label="End", color=blue];
    CO->end[label="End", color=blue];
}
